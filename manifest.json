{
  "name": "Telex redirect",
  "version": "1.0",
  "description": "Prevents you from accidentally visiting index.hu",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "*://*.index.hu/",
    "webRequestBlocking"
  ]
}